  "scripts": {
    "start": "PORT=3002 react-scripts start",
    "start:ci": "npx light-server -s build -p 3002 -b 127.0.0.1 --quiet --historyindex '/index.html'",
    "build": "react-scripts build",
    "build:replace:version": "npx replace-in-file appVersion $BUILD_NUMBER build/index.html",
    "build:replace:branch": "npx replace-in-file appBranch $BRANCH_NAME build/index.html",
    "test": "react-scripts test",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "cypress:ci": "npx start-server-and-test start:ci http://localhost:3002 cypress:run",
    "eject": "react-scripts eject",
    "mock-server": "node ./src/mock-server.js",
    "clean": "rm -rf build && rm -rf node_modules"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:8080",
  "devDependencies": {
    "@aam/eslint-config-aam": "^1.2.1",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "json-server": "^0.14.2",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0"
  }
