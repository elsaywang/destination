{
  "name": "portal-signals-client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@react/react-spectrum": "^2.4.0",
    "babel-preset-react-app": "^3.1.1",
    "classnames": "^2.2.5",
    "cypress": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-react-intl-messages": "^0.8.2",
    "jest-cli": "^22.4.0",
    "jest-junit-reporter": "^1.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "react-scripts-cssmodules": "^1.0.171",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "serve": "^6.5.3",
    "start-server-and-test": "^1.0.1"
  },
  "scripts": {
    "start": "node_modules/serve/bin/serve.js -C -n -l -p 3000 build",
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged",
    "cypress:open": "node_modules/.bin/cypress open",
    "cypress:run": "node_modules/.bin/cypress run",
    "ci": "node_modules/.bin/start-server-and-test start http://localhost:3000 cypress:run",
    "lint": "node_modules/@aam/eslint-config-aam/node_modules/eslint/bin/eslint.js src",
    "json-server": "json-server data-generator.js --port 3002 --routes ./routes.json",
    "externalize": "rm -f ./src/lib/i18n/messages/en.json && NODE_ENV=development node_modules/.bin/extract-messages -l=en --flat true -o ./src/lib/i18n/messages './src/**/!(*.test).js'"
  },
  "proxy": "http://localhost:3002",
  "devDependencies": {
    "@aam/eslint-config-aam": "^1.2.0",
    "eslint-plugin-prettier": "^2.6.0",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "json-server": "^0.12.1",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.2.3"
  }
}
