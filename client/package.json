{
  "name": "portal-signals-client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@aam/aam-topnav": "1.1.1",
    "@react/react-spectrum": "~2.9.0",
    "@spectrum/spectrum-variables": "^1.0.1",
    "babel-preset-react-app": "^3.1.1",
    "classnames": "^2.2.5",
    "coral-shell-ims": "git+ssh://git@git.corp.adobe.com:AAM/coral-shell-ims#v2.0.0",
    "cpx": "^1.5.0",
    "cypress": "^2.1.0",
    "debounce": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-react-intl-messages": "^0.8.2",
    "he": "^1.1.1",
    "http-server": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit-reporter": "^1.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "mockdate": "^2.0.2",
    "moment": "^2.22.1",
    "nodelist-foreach-polyfill": "^1.2.0",
    "npx": "^10.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-app-rewire-define-plugin": "^1.0.0",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.1",
    "react-scripts-cssmodules": "^1.0.171",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.5.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "replace-in-file": "^3.4.0",
    "start-server-and-test": "^1.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:ci": "npx http-server build -p 3000",
    "build": "react-app-rewired build && npm run copy-shell-topnav",
    "build:replace:version": "npx replace-in-file signalsVersion $BUILD_NUMBER build/index.html",
    "build:replace:branch": "npx replace-in-file signalsBranch $BRANCH_NAME build/index.html",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "pretty-quick --staged",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "cypress:ci": "npx start-server-and-test start:ci http://localhost:3000 cypress:run",
    "lint": "node_modules/@aam/eslint-config-aam/node_modules/eslint/bin/eslint.js src",
    "copy-coral-shell-ims": "npx cpx -v './node_modules/coral-shell-ims/build/js/{libs/*js,*.js}' ./build/static/coral-shell-ims/js && npx cpx -v ./node_modules/coral-shell-ims/build/css/*.css ./build/static/coral-shell-ims/css && npx cpx -v './node_modules/coral-shell-ims/build/resources/coralui-component-icon/AdobeSpectrumIcons*.{ttf,woff,woff2}' ./build/static/coral-shell-ims/resources/coralui-component-icon",
    "copy-topnav": "npx cpx -v ./node_modules/@aam/aam-topnav/build/css/*.css ./build/static/css",
    "copy-shell-topnav": "npm run copy-coral-shell-ims && npm run copy-topnav",
    "json-server": "json-server data-generator.js --port 3002 --routes ./routes.json",
    "externalize": "rm -f ./src/lib/i18n/messages/en.json && NODE_ENV=development npx extract-messages -l=en --flat true -o ./src/lib/i18n/messages './src/**/!(*.test).js'"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@aam/eslint-config-aam": "^1.2.1",
    "eslint-plugin-prettier": "^2.6.0",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "json-server": "^0.12.1",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.2.3"
  }
}
